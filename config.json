{
	"cat":
	{
		"cmd": "cat",
		"numprocs": 2,
		"umask": "777",
		"workingdir": "/tmp",
		"autostart": true,
		"autorestart": "unexpected",
		"exitcodes": [0, 2],
		"startretries": 3,
		"starttime": 5,
		"stopsignal": "TERM",
		"stoptime": 10,
		"stdout": "/tmp/env.stdout",
		"stderr": "/tmp/env.stderr",
		"env": {
			"STARTED_BY" : "taskmaster", "ANSWER": 42
		}
	}
}
